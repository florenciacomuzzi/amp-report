{
  "name": "amp-report",
  "version": "1.0.0",
  "description": "AMP Report - AI-powered tenant profile and amenity recommendations for multifamily properties",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon",
    "build": "tsc",
    "build:all": "tsc && cd client && npm run build",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "db:seed": "ts-node src/seeders/index.ts",
    "db:reset": "ts-node src/seeders/index.ts",
    "client:install": "cd client && npm install",
    "client:dev": "cd client && npm start",
    "client:build": "cd client && npm run build",
    "dev:all": "concurrently \"npm run dev\" \"npm run client:dev\"",
    "install:all": "npm install && npm run client:install",
    "present:mvp": "reveal-md slides/mvp.md",
    "generate:trpc-types": "ts-node scripts/generate-trpc-types.ts",
    "postbuild": "npm run generate:trpc-types"
  },
  "keywords": ["real-estate", "property-management", "tenant-analysis", "amenities"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "sequelize": "^6.35.2",
    "sequelize-typescript": "^2.1.6",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "axios": "^1.6.5",
    "@googlemaps/google-maps-services-js": "^3.3.42",
    "openai": "^4.24.7",
    "pdfkit": "^0.14.0",
    "multer": "^1.4.5-lts.1",
    "winston": "^3.11.0",
    "uuid": "^9.0.1",
    "reflect-metadata": "^0.2.1",
    "@trpc/server": "^10.0.0",
    "@trpc/client": "^10.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.7",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/pdfkit": "^0.13.3",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "sequelize-cli": "^6.6.2",
    "concurrently": "^8.2.2",
    "reveal-md": "^6.1.4"
  }
}